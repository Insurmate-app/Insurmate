{
  "source": {
    "include": ["./"],
    "exclude": ["node_modules", "dist", "docs"],
    "includePattern": ".+\\.js$"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/docdash",
    "readme": "./README.md"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "tags": {
    "allowUnknownTags": true
  }
}
